; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pengview"
config/version="1.3.0"
run/main_scene="res://scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="PspriteGames/Pengview"
config/project_settings_override="user://override.cfg"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/low_processor_mode=true
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

InitialSetup="*res://scripts/initial_setup.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=500
window/size/initial_position_type=4
window/size/borderless=true
window/size/transparent=true
window/energy_saving/keep_screen_on=false
window/subwindows/embed_subwindows=false
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true
window/handheld/orientation=6
window/ios/allow_high_refresh_rate=false

[gui]

timers/tooltip_delay_sec=0.05

[psprite_games]

pengview/check_for_depends=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
viewport/transparent_background=true
